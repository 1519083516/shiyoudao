{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {}
		}, {
			"path": "pages/application/application",
			"style": {
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/space/space",
			"style": {
				"enablePullDownRefresh": false
			}
		},{
			"path": "pages/information/information",
			"style": {
				"enablePullDownRefresh": false
			}
		},{
			"path": "pages/me/me",
			"style": {
				"enablePullDownRefresh": false
			}
		}],
		"tabBar": {
			"color": "#C7C7C7",
			"selectedColor": "#515151",
			"borderStyle": "white",
			"backgroundColor": "#FFF9F1",
			"fontSize": "10px",
			"list": [{
				"pagePath": "pages/index/index",
				"iconPath": "static/topbar-icon/index_0.png",
				"selectedIconPath": "static/topbar-icon/index_1.png",
				"text": "首页"
			}, {
				"pagePath": "pages/application/application",
				"iconPath": "static/topbar-icon/application_0.png",
				"selectedIconPath": "static/topbar-icon/application_1.png",
				"text": "应用"
			}, {
				"pagePath": "pages/space/space",
				"iconPath": "static/topbar-icon/space_0.png",
				"selectedIconPath": "static/topbar-icon/space_1.png",
				"text": "空间"
			},{
				"pagePath": "pages/information/information",
				"iconPath": "static/topbar-icon/information_0.png",
				"selectedIconPath": "static/topbar-icon/information_1.png",
				"text": "消息"
			}, {
				"pagePath": "pages/me/me",
				"iconPath": "static/topbar-icon/me_0.png",
				"selectedIconPath": "static/topbar-icon/me_1.png",
				"text": "我的"
			}]
		},
		"globalStyle": {
			"navigationStyle": "custom"
		}
	}
